@model IEnumerable<n01629177Cumulative1.Models.Course>

@if(Model.Count() > 0){
  <div>
    <p>The following courses will be affected by this action : </p>

    @foreach(var course in Model){
        <div>
            <a href="/Course/Show?class_id=@course.classId">
                @course.classCode : @course.className
            </a>
        </div>
    }

    <div>&nbsp;</div>
  </div>
}
